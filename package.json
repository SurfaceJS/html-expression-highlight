{
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "grammars": [
            {
                "injectTo": [
                    "text.html.derivative"
                ],
                "path": "./syntaxes/html-expression.tmLanguage.json",
                "scopeName": "text.html.expression"
            },
            {
                "injectTo": [
                    "text.html.derivative"
                ],
                "path": "./syntaxes/html-attribute-expression.tmLanguage.json",
                "scopeName": "text.html.attribute-expression"
            },
            {
                "injectTo": [
                    "text.html.derivative"
                ],
                "path": "./syntaxes/html-interpolation-expression.tmLanguage.json",
                "scopeName": "text.html.interpolation-expression"
            }
        ]
    },
    "description": "Surface's html expressions highlight",
    "displayName": "Html Expressions highlight",
    "engines": {
        "vscode": "^1.40.0"
    },
    "name": "surface.html-expression-highlight",
    "version": "0.0.1"
}